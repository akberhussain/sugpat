<% include ./partials/header %>
		
	<div class="row">
                        <div class="col-lg-10">

                            <!-- Traffic sources -->
                            <div class="panel panel-flat">
                                <div class="panel-heading">
                                    <h1 class="panel-title">Registered Patients</h1>
                                    <div class="heading-elements">
                                        <span><i class="icon-bell2"></i></span>
                                    </div>
                                </div>

                                <div class="container-fluid">
                                    <div class="row">
                                        

                                        <div class="col-lg-12">
 											 <table class = "table table-striped table-bordered table-hover table-responsive box-shadow-table">
                                                <thead class = "thead-inverse">
                                                	
                                                	<tr>
									                    <th><h4><strong>Name</strong></h4></th>
									                    <th><h4><strong>Email</strong></h4></th>
									                    <th><h4><strong>CNIC</strong></h4></th>
									                    <th><h4><strong>Contact</strong></h4></th>
									                    <th><h4><strong>Gender</strong></h4></th>
									                </tr>
                                               	</thead>
                                               	<tbody>
                                               		<% patients.forEach(function(patient){ %>
                                               			<tr>
                                               				<td><strong><%= patient.name %></strong></td>
                                               				<td><strong><%= patient.username %></strong></td>
                                               				<td><strong><%= patient.cnic %></strong></td>
                                               				<td><strong><%= patient.num %></strong></td>
                                               				<td><strong><%= patient.gender %></strong></td>
                                               			</tr>
                                                	<% }); %>
                                               	</tbody>		
                                             </table> 	
                                        </div>

                                    </div>
                                </div>

                                <div class="position-relative" id=""></div>
                            </div>
                            <!-- /traffic sources -->
                        </div>
                    </div>   	

<% include ./partials/footer %>