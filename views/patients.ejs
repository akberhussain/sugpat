<% include ./partials/header %>
		
	<div class="row">
                        <div class="col-lg-12">

                            <!-- Traffic sources -->
                            <div class="panel panel-flat">
                                <div class="panel-heading">
                                    <h1 class="panel-title">Registered Patients</h1>
                                    <div class="heading-elements">
                                        <span><i class="icon-bell2"></i></span>
                                    </div>
                                </div>

                                <div class="container-fluid">
                                    <div class="row">
                                        

                                        <div class="col-lg-12">
 											 <table class = "table table-striped table-bordered table-hover table-responsive box-shadow-table">
                                                <thead class = "thead-inverse">
                                                	
                                                	<tr>
									                    <th><h4><strong>Name</strong></h4></th>
									                    <th><h4><strong>React</strong></h4></th>

									                </tr>
                                               	</thead>
                                               	<tbody>
                                               		<% patients.forEach(function(patient){ %>
                                               			<tr>
                                                      <td><h5><a href="patient<%= patient._id %>" ><%= patient.name %></a></h5></td>
                                               				
                                                      <td>
                                                        <form action = "/removepatient<%= patient.id %>?_method=delete" method = "post">
                                                          <input type = "submit" class = "btn btn-danger btn-xs" value = "Remove Patient">
                                                        </form>
                                                      </td>
                                               			</tr>
                                                	<% }); %>
                                               	</tbody>		
                                             </table> 	
                                        </div>

                                    </div>
                                </div>

                                <div class="position-relative" id=""></div>
                            </div>
                            <!-- /traffic sources -->
                        </div>
                    </div>   	

<% include ./partials/footer %>