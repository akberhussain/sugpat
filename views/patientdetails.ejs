<% include ./partials/dheader %>

        <div class="row">
        <div class="col-lg-12">

                            <!-- Traffic sources -->
                            <div class="panel panel-flat">
                                <div class="panel-heading">
                                    <h1 class="panel-title"><%= patient.name %>'s Information</h1>
                                    <div class="heading-elements">
                                        <span><i class="icon-bell2"></i></span>
                                    </div>
                                </div>

                                <div class="container-fluid">
                                    <div class="row">
                                        

                                        <div class="col-lg-12">
                                            <table class = "table table-striped table-bordered table-hover table-responsive box-shadow-table">

                                                 <thead class = "thead-inverse">
                                                    <tr><th><h5>Name</h5></th><td><strong><%= patient.name %></strong></td></tr>
                                                    <tr><th><h5>Email</h5></th><td><strong><%= patient.username %></strong></td></tr>
                                                    <tr><th><h5>CNIC  No</h5></th><td><strong><%= patient.cnic %></strong></td></tr>
                                                    <tr><th><h5>Age</h5></th><td><strong><%= patient.age %></strong></td></tr>
                                                    <tr><th><h5>Gender</h5></th><td><strong><%= patient.gender %></strong></td></tr>
                                                    <tr><th><h5>Conatct</h5></th><td><strong><%= patient.num %></strong></td></tr>
                                                    <tr><th><h5>Blood Group</h5></th><td><strong><%= patient.bloodgroup %></strong></td></tr>
                                                    <tr><th><h5>Diabetes Duration</h5></th><td><strong><%= patient.diabetesduration %></strong></td></tr>
                                                    <tr><th><h5>Drug History</h5></th><td><strong><%= patient.drughistory %></strong></td></tr>
                                                    <tr><th><h5>Heridity</h5></th><td><strong><%= patient.heridity %></strong></td></tr>
                                                </thead>
                                                <tbody>
                                                    <tr></tr>
                                                </tbody>    
                                            </table>        

                                        </div>

                                    </div>
                                </div>

                                <div class="position-relative" id=""></div>
                            </div>
                            <!-- /traffic sources -->
                        </div>
    </div>  

<% include ./partials/footer %>



