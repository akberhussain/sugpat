<% include ./partials/header %>

	<div class="row">
                        <div class="col-lg-10">

                            <!-- Traffic sources -->
                            <div class="panel panel-flat">
                                <div class="panel-heading">
                                    <h1 class="panel-title">Registered Doctors</h1>
                                    <div class="heading-elements">
                                        <span><i class="icon-bell2"></i></span>
                                    </div>
                                </div>

                                <div class="container-fluid">
                                    <div class="row">
                                        

                                        <div class="col-lg-12">
 											 <table class = "table table-striped table-bordered table-hover table-responsive box-shadow-table">
                                                <thead class = "thead-inverse">
                                                	
                                                	<tr>
									                    <th><h4><strong>Name</strong></h4></th>
									                    <th><h4><strong>Email</strong></h4></th>
									                    <th><h4><strong>CNIC</strong></h4></th>
									                    <th><h4><strong>Contact</strong></h4></th>
									                    <th><h4><strong>Gender</strong></h4></th>
									                    <th><h4><strong>Qualification</strong></h4></th>
									                    <th><h4><strong>Specialization</strong></h4></th>
									                </tr>
                                               	</thead>
                                               	<tbody>
                                               		<% doctors.forEach(function(doctor){ %>
                                               			<tr>
                                               				<td><strong><%= doctor.name %></strong></td>
                                               				<td><strong><%= doctor.username %></strong></td>
                                               				<td><strong><%= doctor.cnic %></strong></td>
                                               				<td><strong><%= doctor.num %></strong></td>
                                               				<td><strong><%= doctor.gender %></strong></td>
                                               				<td><strong><%= doctor.qualification %></strong></td>
                                               				<td><strong><%= doctor.specialization %></strong></td>
                                               			</tr>
                                                	<% }); %>
                                               	</tbody>		
                                             </table> 	
                                        </div>

                                    </div>
                                </div>

                                <div class="position-relative" id=""></div>
                            </div>
                            <!-- /traffic sources -->
                        </div>
                    </div>

<% include ./partials/footer %>